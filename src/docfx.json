{
  "metadata": [
    {
      "src": [
        {
          "src": "../../abbot/src",
          "files": [
            "**/Abbot.Scripting.Interfaces.csproj"
          ]
        }
      ],
      "dest": "api-csharp",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api-csharp/**.yml"
        ]
      },
      {
        "files": [
          "api/*.md",
          "guides/**.md",
          "guides/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "templates/abbot/*.svg",
          "templates/abbot/*.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../_site",
    "globalMetadata": {
      "_appTitle": "Abbot Docs",
      "_appLogoPath": "templates/abbot/logo.svg",
      "_appFooter": "Copyright Â© A Serious Business, Inc.",
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/abbot"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}