{
  "metadata": [
    {
      "src": [
        {
          "src": "../../abbot/src",
          "files": [
            "**/Abbot.Scripting.Interfaces.csproj"
          ]
        }
      ],
      "dest": "api-csharp",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api-csharp/**.yml"
        ]
      },
      {
        "files": [
          "api-py/**.yml"
        ]
      },
      {
        "files": [
          "api/index.md",
          "api/toc.yml"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "templates/abbot/*.svg",
          "templates/abbot/*.ico",
          "CNAME"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "../_site",
    "globalMetadata": {
      "_appTitle": "Abbot Docs",
      "_appFooter": "Copyright Â© A Serious Business, Inc.",
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/abbot"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
